#!/usr/bin/bash
set -e # exit on error

# Load environment variables
source $(dirname $0)/../.env

printf "\033[1;35m == db-setup rds-psql-db \033[0m\n"

# Use RDS connection for all scripts
export CONNECTION_URL="$CONNECTION_URL_DB"

# Run create, schema-load, seed scripts
source "./create"
source "./schema-load"
source "./seed"
