#!/usr/bin/env python3

import requests
import os
from dotenv import load_dotenv

load_dotenv()

try:
    response = requests.get(f"{os.getenv('BACKEND_URL')}/api/health-check", timeout=5)
    # print("backendurl", os.getenv('BACKEND_URL'))
    print("FLASK is HEALTHY" if response.status_code == 200 else "UNHEALTHY")
except:
    print("FLASK is UNHEALTHY")
