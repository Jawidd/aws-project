#! /usr/bin/bash

source $(dirname $0)/../.env

if [ "$ENV_TYPE" = "prod" ]; then
    printf "\033[1;35m == db-seed rds-psql-db \033[0m\n " 
    psql $AWS_RDS_PSQL_CONNECTION cruddur < $(dirname $0)/../../db/seed.sql
else
    printf "\033[1;36m == db-seed localhost-postgresql \033[0m\n " 
    psql $CONNECTION_URL cruddur < $(dirname $0)/../../db/seed.sql
fi


